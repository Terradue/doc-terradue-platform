
/*#page {min-height:600px;}*/
.jsonView .package {margin-left:10px;padding:3px;border-radius:2px;margin-top:2px;}
.jsonView .header {cursor:pointer;}

.jsonView .name {color:gray;}

.jsonView .array {background-color:#FFD8BB;border:thin solid #FFB780;}
.jsonView .object {background-color:#E7F1FE;border:thin solid #BBD0E9;}

.jsonView .string {color:#b94a48;} /* tw bootstrap text-error */
.jsonView .number {color:#3a87ad;} /* tw bootstrap text-info */
.jsonView .function {color:#468847;} /* tw bootstrap text-success */

.jsonView .open .children {display:block;}
.jsonView .closed .children {display:none;}

.jsonView .arrow {background-image:url("../img/d.png"); background-repeat:no-repeat; background-color:transparent; height:15px; width:15px; display:inline-block;}

.jsonView .open .arrow {background-position:-20px 0;}
.jsonView .closed .arrow {background-position:0 0;}

.jsonView .type {color:gray;font-size:8pt;float:right;}

.jsonView .hide {display:none;}

